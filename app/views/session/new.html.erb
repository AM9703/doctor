<h1>Login</h1>
<div>
  <div>
    <%= form_with url: session_index_path, scope: :session do |f| %>
      <div>
        <%= f.label :email %>
        <%= f.email_field :email%><br>
      </div>
      <div>
        <%= f.label :password %>
        <%= f.password_field :password%><br>
      </div>
        <%= f.submit "Log in", method: :index  %>
    <% end %>
    <%= link_to "Sign up", new_user_path %>
  </div>
</div>