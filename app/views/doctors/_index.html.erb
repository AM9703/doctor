<h2>Doctor's Appointments</h2 >
<h4>Appointments</h4>
  <table border="1px">
    <thead>
      <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Status</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% @appointments.each do |appointment|%>
        <td><%= appointment.date%></td>
        <td><%= appointment.time%></td>
        <td><%= appointment.status %></td>
        <td><%=link_to 'Complete', appointment_path(appointment, appointment: {status: 'Complete'}), method: :put%></td> 
        <td><%=link_to 'cancle', appointment_path(appointment, appointment: {status: 'cancle'}), method: :put %></td>
      </tr>
      <%end%>
    </tbody>
  </table>
<%= link_to "Log out", new_session_path, method: :delete %>
