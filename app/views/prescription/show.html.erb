<form class="row gx-3 gy-2 align-items-center  border border-1 mt-5">
  <h1>Prescription </h1>
  <%if @prescription.present?%>
    <p><strong>Prescription : </strong><%=@prescription.prescription%></p>
  <%else%>
    <p>No prescription </p>
  <%end%>
  <div  class="mt-1 mb-2">
    <%=link_to 'Back',root_path, class: "btn btn-light "%>  
  </div>
</form>

