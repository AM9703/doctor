<h1>Patient's</h1>
<%=link_to 'New Patient', new_patient_path%>
<table>
	<thead>
		<tr>
			
			<th>Name</th>
			<th>Age</th>
			<th>Gender</th>
			<th colspan="3">Action</th>
		</tr>
	</thead>
	<tbody>
		<% @patients.each do |patient|%>
		<tr>

			<td><%= patient.name%></td>
			<td><%= patient.age%></td>
			<td><%= patient.gender%></td>
      <td><%=link_to 'View',patient%></td>
			
		</tr>
		<%end%>
	</tbody>
</table>